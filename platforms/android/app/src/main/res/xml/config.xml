<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.hello" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>Kata</name>
    <description>
        An Adam App.
    </description>
    <author email="adam15free@hotmail.com" href="http://localhost">
        Adam Holmes
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/skc-Logo.png" />
    <allow-intent href="market:*" />
    <splash density="land-hdpi" src="www/img/skc-Logo.png" />
    <splash density="land-ldpi" src="www/img/skc-Logo.png" />
    <splash density="land-mdpi" src="www/img/skc-Logo.png" />
    <splash density="land-xhdpi" src="www/img/skc-Logo.png" />
    <splash density="port-hdpi" src="www/img/skc-Logo.png" />
    <splash density="port-ldpi" src="www/img/skc-Logo.png" />
    <splash density="port-mdpi" src="www/img/skc-Logo.png" />
    <splash density="port-xhdpi" src="www/img/skc-Logo.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
